                                  <span class="symbol-label">
                                    <i class="ki-solid ki-note fs-1 text-primary-success"> </i>
                                  </span>
                                </div>
                                <!--end::Symbol-->

                                <!--begin::Stats-->
                                <div class="m-0">
                                  <!--begin::Number-->
                                  <span class="text-gray-700 fw-bolder d-block fs-2qx lh-1 ls-n1 mb-1"><%= totalTag %></span>
                                  <!--end::Number-->

                                  <!--begin::Desc-->
                                  <span class="text-gray-500 fw-semibold fs-6">Tag</span>
                                  <!--end::Desc-->
                                </div>
                                <!--end::Stats-->
                              </div>
                              <!--end::Items-->
                            </a>
                          </div>
                          <!--end::Col-->
                          <!--begin::Col-->
                          <div class="col-2">
                            <a href="<%= process.env.BASE_URL %>ticket">
                              <!--begin::Items-->
                              <div class="bg-gray-100 bg-opacity-70 rounded-2 px-6 py-5">
                                <!--begin::Symbol-->
                                <div class="symbol symbol-30px me-5 mb-8">
                                  <span class="symbol-label">
                                    <i class="ki-solid ki-calendar-tick fs-1 text-primary-success"></i>
                                  </span>
                                </div>
                                <!--end::Symbol-->

                                <!--begin::Stats-->
                                <div class="m-0">
                                  <!--begin::Number-->
                                  <span class="text-gray-700 fw-bolder d-block fs-2qx lh-1 ls-n1 mb-1"><%= totalTicket %></span>
                                  <!--end::Number-->

                                  <!--begin::Desc-->
                                  <span class="text-gray-500 fw-semibold fs-6">Total Ticket</span>
                                  <!--end::Desc-->
                                </div>
                                <!--end::Stats-->
                              </div>
                              <!--end::Items-->
                            </a>
                          </div>
                          <!--end::Col-->
                          <!--begin::Col-->
                          <div class="col-2">
                            <a href="<%= process.env.BASE_URL %>ticket">
                              <!--begin::Items-->
                              <div class="bg-gray-100 bg-opacity-70 rounded-2 px-6 py-5">
                                <!--begin::Symbol-->
                                <div class="symbol symbol-30px me-5 mb-8">
                                  <span class="symbol-label">
                                    <i class="ki-solid ki-calendar-tick fs-1 text-primary-success"></i>
                                  </span>
                                </div>
                                <!--end::Symbol-->

                                <!--begin::Stats-->
                                <div class="m-0">
                                  <!--begin::Number-->
                                  <span class="text-gray-700 fw-bolder d-block fs-2qx lh-1 ls-n1 mb-1"><%= totalUpcomingTicket %></span>
                                  <!--end::Number-->

                                  <!--begin::Desc-->
                                  <span class="text-gray-500 fw-semibold fs-6">Upcoming Event Ticket</span>
                                  <!--end::Desc-->
                                </div>
                                <!--end::Stats-->
                              </div>
                              <!--end::Items-->
                            </a>
                          </div>
                          <!--end::Col-->
                          <!--begin::Col-->
                          <div class="col-2">
                            <a href="<%= process.env.BASE_URL %>past-ticket">
                              <!--begin::Items-->
                              <div class="bg-gray-100 bg-opacity-70 rounded-2 px-6 py-5">
                                <!--begin::Symbol-->
                                <div class="symbol symbol-30px me-5 mb-8">
                                  <span class="symbol-label">
                                    <i class="ki-solid ki-calendar-tick fs-1 text-primary-success"> </i>
                                  </span>
                                </div>
                                <!--end::Symbol--
                                <!--begin::Stats-->
                                <div class="m-0">
                                  <!--begin::Number-->
                                  <span class="text-gray-700 fw-bolder d-block fs-2qx lh-1 ls-n1 mb-1"><%= totalPastTicket %></span>
