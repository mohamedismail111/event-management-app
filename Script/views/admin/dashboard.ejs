                      <!--begin::Title-->
                      <h3 class="card-title align-items-start flex-column">
                        <span class="card-label fw-bold text-gray-900">Recent Ticket</span>
                        <span class="text-gray-500 mt-1 fw-semibold fs-6">
                          Total <%= totalTicket %> ticket, with <%= totalPastTicket %> past ticket
                        </span>
                      </h3>
                      <!--end::Title-->
                      <!--begin::Toolbar-->
                      <div class="card-toolbar">
                        <a href="<%= process.env.BASE_URL %>ticket" class="btn btn-sm btn-primary">View More</a>
                      </div>
                      <!--end::Toolbar-->
                    </div>
                    <!--end::Card header-->
                    <!--begin::Card body-->
                    <div class="card-body">
                      <!--begin::Table-->
                      <div id="kt_table_widget_5_table_wrapper" class="dataTables_wrapper dt-bootstrap4 no-footer">
                        <div class="table-responsive">
                          <table class="table align-middle table-row-dashed fs-6 gy-6 dataTable no-footer" id="kt_table_widget_5_table">
                            <!--begin::Table head-->
                            <thead>
                              <!--begin::Table row-->
                              <tr class="text-start text-gray-500 fw-bold fs-7 text-uppercase gs-0">
                                <th class="min-w-100px">Ticket No</th>
                                <th class="min-w-100px">user name</th>
                                <th class="min-w-100px">event</th>
                                <th class="min-w-100px">event date</th>
                                <th class="min-w-50px">ticket</th>
                                <th class="min-w-50px">price</th>
                                <th class="min-w-50px">total</th>
                              </tr>
                              <!--end::Table row-->
                            </thead>
                            <!--end::Table head-->

                            <!--begin::Table body-->
                            <tbody class="fw-bold text-gray-600">
                              <% if (tickets.length > 0) { %>
                              <% for (let i = 0; i < tickets.length; i++) { %>
                              <!-- begin::tr -->
                              <tr>
                                <td class="fw-bolder text-gray-800"><%= tickets[i].ticket_number %></td>
                                <td class="fw-bolder text-gray-800"><%= tickets[i].userId ? tickets[i].userId.name : "N/A" %></td>
                                <td class="fw-bolder text-gray-800"><%= tickets[i].eventId ? tickets[i].eventId.event : "N/A" %></td>
                                <td class="fw-bolder text-gray-800">
                                  <%= new Date(`${tickets[i].eventId.date}`).toLocaleDateString('en-US', { year: 'numeric', month: 'short', day: 'numeric'}) %>
                                </td>
                                <td class="fw-bolder text-primary"> <%= tickets[i].total_ticket %> </td>
                                <td class="fw-bolder text-primary"><%= tickets[i].price %></td>
                                <td class="fw-bolder text-success"><%= tickets[i].total_amount %></td>
                              </tr>
                              <% } %>
                              <% } else { %>
                              <tr>
                                <td class="text-center"> No recent tickets </td>
                              </tr>
                              <% } %>
                            </tbody>
                            <!--end::Table body-->
                          </table>
                        </div>
                        <div class="row">
                          <div class="col-sm-12 col-md-5 d-flex align-items-center justify-content-center justify-content-md-start"></div>
                          <div class="col-sm-12 col-md-7 d-flex align-items-center justify-content-center justify-content-md-end"></div>
                        </div>
                      </div>
                      <!--end::Table-->
                    </div>
                    <!--end::Card body-->
                  </div>
                  <!--end::card-->
                </div>
                <!--end::Col-->
                <!--begin::Col-->
                <div class="col-xl-6">
                 <!--begin::Table Widget 5-->
                 <div class="card card-flush h-xl-100">
                    <!--begin::Card header-->
                    <div class="card-header pt-7">
                      <!--begin::Title-->
                      <h3 class="card-title align-items-start flex-column">
                        <span class="card-label fw-bold text-gray-900">Upcoming Event </span>
                        <span class="text-gray-500 mt-1 fw-semibold fs-6">
                          Total <%= totalEvent %> event
                        </span>
                      </h3>
                      <!--end::Title-->
                      <!--begin::Toolbar-->
                      <div class="card-toolbar">
                        <a href="<%= process.env.BASE_URL %>event" class="btn btn-sm btn-primary">View More</a>
                      </div>
                      <!--end::Toolbar-->
                    </div>
                    <!--end::Card header-->
                    <!--begin::Card body-->
                    <div class="card-body">
                      <!--begin::Table-->
                      <div id="kt_table_widget_5_table_wrapper" class="dataTables_wrapper dt-bootstrap4 no-footer">
                        <div class="table-responsive">
                          <table class="table align-middle table-row-dashed fs-6 gy-6 dataTable no-footer" id="kt_table_widget_5_table">
                            <!--begin::Table head-->
                            <thead>
                              <!--begin::Table row-->
                              <tr class="text-start text-gray-500 fw-bold fs-7 text-uppercase gs-0">
                                <th class="min-w-100px">Name</th>
                                <th class="min-w-80px">Date</th>
                                <th class="min-w-60px">Time</th>
                                <th class="min-w-60px">Price</th>
                                <th class="min-w-40px">Total Seat</th>
                                <th class="min-w-40px">Booked</th>
                                <th class="min-w-40px">ava.seat</th>
                              </tr>
                              <!--end::Table row-->
                            </thead>
                            <!--end::Table head-->
                            <!--begin::Table body-->
                            <tbody class="fw-bold text-gray-600">
                              <% if (events.length > 0) { %>
                              <% for (let i = 0; i < events.length; i++) { %>
                              <!-- begin::tr -->
                              <tr>
                                <td class="fw-bolder text-gray-800"><%= events[i].event %></td>
                                <td class="fw-bolder text-gray-800">
                                  <%= new Date(`${events[i].date}`).toLocaleDateString('en-US', { year: 'numeric', month: 'short', day: 'numeric'}) %>
                                </td>
                                <td class="fw-bolder text-gray-800"><%= events[i].time %></td>
                                <td class="fw-bolder text-gray-800"><%= events[i].price %></td>
                                <td class="fw-bolder text-primary"><%= events[i].totalSeat %></td>
                                <td class="fw-bolder text-success"><%= events[i].totalBookedTicket %></td>
                                <td class="fw-bolder text-danger"><%= events[i].availableticket %></td>
                              </tr>
                              <% } %>
                              <% } else { %>
                              <tr>
                                <td class="text-center"> No recent events </td>
                              </tr>
                              <% } %>
                            </tbody>
                            <!--end::Table body-->
                          </table>
                        </div>
                      </div>
                      <!--end::Table-->
                    </div>
                    <!--end::Card body-->
                 </div>
                 <!--end::Table Widget 5-->
                </div>
                <!--end::Col-->
              </div>
              <!-- end:: second row -->
            </div>
            <!--end::Content container-->
          </div>
