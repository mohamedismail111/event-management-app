                            <a href="<%= process.env.BASE_URL %>user">
                              <!--begin::Items-->
                              <div class="bg-gray-100 bg-opacity-70 rounded-2 px-6 py-5">
                                <!--begin::Symbol-->
                                <div class="symbol symbol-30px me-5 mb-8">
                                  <span class="symbol-label">
                                    <i class="ki-solid ki-user-square fs-1 text-primary-success"> </i>
                                  </span>
                                </div>
                                <!--end::Symbol--
                                <!--begin::Stats-->
                                <div class="m-0">
                                  <!--begin::Number-->
                                  <span class="text-gray-700 fw-bolder d-block fs-2qx lh-1 ls-n1 mb-1"><%= totalUser %></span>
